# # operators 
# 1 Arithmetic 2) relational 3) logical 4) assignment 
# Arithmetic -->> +,-,*,/,%%,%/%,^ or **
#   c<-10
# d<-20
# c+d
# c-d
# c*d
# c/d
# d/c
# c%%d
# d%%c
# c%/%d
# d%/%c
# c^d

#  Relational 
#<,>,==,<=,>=,!=  always trues or FALSE
# print(a>b)
# print(a<b)
# print(a==b)
# print(a<=b)
# print(a>=b)
# print(a!=b)
# a1<-c(1,2,3,4)
# b1<-c(3,4,5,6,7)
# print(a1>b1)
# print(a1<b1)
#  print(a1==b1)
#  print(a1<=b1)
