#  print(a1!=b1)


# LOgical 
singe and 
or 
not 
double and
double or 

a1<-c(1,2.4,TRUE,4+3i)
b1<-c(0,4,TRUE,6+8i)
print(a1&b1)
print(c&&d)
print(c&d)


  
